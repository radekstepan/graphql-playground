    "jest": "29.3.1",
    "parcel": "^2.8.0",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@apollo/server": "^4.1.1",
    "concurrently": "^7.5.0",
    "graphql": "^16.6.0",
